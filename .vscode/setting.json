    "class:\\s*if[^{]*\\{[^}]*\\}\\s*else\\s*\\{\\s*\"([^\"]*)\"[^}]*\\}",
    "class:\\s*if[^{]*\\{\\s*\"([^\"]*)\"",
    "else\\s*\\{\\s*\"([^\"]*)\"[^}]*\\}"
